{
  "name": "acf/postlist",
  "title": "Posts List",
  "description": "A custom block for displaying an archive of posts (or custom post types).",
  "textdomain": "cloakwp",
  "category": "widgets",
  "icon": "admin-page",
  "keywords": [
    "cards",
    "posts",
    "pages",
    "tiles",
    "list"
  ],
  "acf": {
    "mode": "edit",
    "renderTemplate": "block-preview.php"
  },
  "align": false,
  "supports": {
    "anchor": true,
    "mode": true,
    "align": false,
    "html": false,
    "reusable": false,
    "lock": false,
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ]
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "attributes": {
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "margin": {
            "top": "80px",
            "bottom": "80px"
          }
        }
      }
    }
  }
}